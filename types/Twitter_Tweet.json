{
    "name": "Twitter_Tweet",
    "naturalKey": [ "id" ],
    "indexes": [
        { 
            "keys": [ "id" ],
            "options": { "unique": true }
        }
    ],
    "properties": {
        "coordinates":                         { "type": "GeoJSON" },
        "created_at":                          { "type": "DateTime" },
        "favorite_count":                      { "type": "Integer" },
        "favorited":                           { "type": "Boolean" },
        "id":                                  { "type": "Integer", "required": true },
        "id_str":                              { "type": "String", "required": true },
        "in_reply_to_screen_name":             { "type": "String" },
        "in_reply_to_status_id":               { "type": "Integer" },
        "in_reply_to_status_id_str":           { "type": "String" },
        "in_reply_to_user_id":                 { "type": "Integer" },
        "in_reply_to_user_id_str":             { "type": "String" },
        "is_quote_status":                     { "type": "Boolean" },
        "lang":                                { "type": "String" },
        "place":                               { "type": "Object" },
        "possibly_sensitive":                  { "type": "Boolean" },
        "retweet_count":                       { "type": "Integer" },
        "retweeted":                           { "type": "Boolean" },
        "source":                              { "type": "String" },
        "text":                                { "type": "String" },
        "truncated":                           { "type": "Boolean" },
        "url":                                 { "type": "String" },
        "user__contributors_enabled":          { "type": "Boolean" },
        "user__created_at":                    { "type": "DateTime" },
        "user__default_profile":               { "type": "Boolean" },
        "user__default_profile_image":         { "type": "Boolean" },
        "user__description":                   { "type": "String" },
        "user__entities":                      { "type": "Object" },
        "user__favourites_count":              { "type": "Integer" },
        "user__follow_request_sent":           { "type": "Boolean" },
        "user__followers_count":               { "type": "Integer" },
        "user__following":                     { "type": "Boolean" },
        "user__friends_count":                 { "type": "Integer" },
        "user__geo_enabled":                   { "type": "Boolean" },
        "user__has_extended_profile":          { "type": "Boolean" },
        "user__id":                            { "type": "Integer" },
        "user__id_str":                        { "type": "String" },
        "user__is_translation_enabled":        { "type": "String" },
        "user__is_translator":                 { "type": "Boolean" },
        "user__lang":                          { "type": "String" },
        "user__listed_count":                  { "type": "Integer" },
        "user__location":                      { "type": "String" },
        "user__name":                          { "type": "String" },
        "user__notifications":                 { "type": "Boolean" },
        "user__profile_image_url":             { "type": "String" },
        "user__profile_image_url_https":       { "type": "String" },
        "user__profile_link_color":            { "type": "String" },
        "user__profile_sidebar_border_color":  { "type": "String" },
        "user__profile_sidebar_fill_color":    { "type": "String" },
        "user__profile_text_color":            { "type": "String" },
        "user__profile_use_background_image":  { "type": "Boolean" },
        "user__protected":                     { "type": "Boolean" },
        "user__screen_name":                   { "type": "String" },
        "user__statuses_count":                { "type": "Integer" },
        "user__time_zone":                     { "type": "String" },
        "user__url":                           { "type": "String" },
        "user__utc_offset":                    { "type": "Integer" },
        "user__verified":                      { "type": "Boolean" }
    }
